<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CentralInovacao.Views.ViewRankingGeral"
             Title="Ranking Geral">

    <Grid RowDefinitions="*,*,*,*">
        <Label  Grid.Row="0"
                Text="Primeiro Lugar"
                FontSize="Large"
                HorizontalOptions="Center" />
        <Label  Grid.Row="1"
                Text="Segundo Lugar"
                FontSize="Large"
                HorizontalOptions="Center" />
        <Label  Grid.Row="2"
                Text="Terceiro Lugar"
                FontSize="Large"
                HorizontalOptions="Center" />
        <Label  Grid.Row="3"
                Text="Quarto Lugar"
                FontSize="Large"
                HorizontalOptions="Center" />
    </Grid>

</ContentPage>